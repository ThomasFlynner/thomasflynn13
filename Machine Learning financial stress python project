   ],
   "source": [
    "def plotting_target_variable(dataset, column_name):  \n",
    "    #plotting the distribution of Target Variable\n",
    "    plt.figure(figsize=(7, 5)) # Setting the figure size\n",
    "    sns.countplot(x=column_name, data=dataset)\n",
    "    plt.title('Distribution of Target Variable') # title for plot\n",
    "    # Getting the exact counts for yes and no\n",
    "    counts = dataset[column_name].value_counts()\n",
    "    # Loop through the counts and add text annotations for counts\n",
    "    for i, count in enumerate(counts):\n",
    "        #Place the count above its bar\n",
    "        plt.text(i, count, str(count), ha='center', va='bottom')\n",
    "    plt.show() #display the plot\n",
    "    \n",
    "#plotting the target variables distribution    \n",
    "plotting_target_variable(dataset,'SeriousDlqin2yrs')"
   ]
  },
