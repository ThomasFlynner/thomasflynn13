    "def summary_of_data(dataset):\n",
    "# Getting the shape \n",
    "    print(dataset.shape)\n",
    "    # to checks if the variable is an instance of a pandas Df\n",
    "    if isinstance(dataset, pd.DataFrame):\n",
    "        print(dataset.columns.tolist())\n",
    "\n",
    "    # displaying the first few rows \n",
    "    display(dataset.head())  \n",
    "    # displaying the last few rows\n",
    "    display(dataset.tail())\n",
    "#getting summary of descriptive features \n",
    "summary_of_data(x)"
   ]
  },
