   "source": [
    "def extract_descriptive_features_and_target(dataset, target_column_name):\n",
    "    \n",
    "    # Getting the descriptive features by dropping target column\n",
    "    x = dataset.drop(target_column_name, axis=1)\n",
    "    \n",
    "    # Getting the target feature\n",
    "    y = dataset[target_column_name]\n",
    "    \n",
    "    return x, y\n",
    "# getting descriptive and target variable using function\n",
    "x, y = extract_descriptive_features_and_target(dataset, 'SeriousDlqin2yrs')"
   ]
  },
